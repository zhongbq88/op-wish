<div id="content" style=" margin:15px;margin-top:30px;">
  <div class="container-fluid">
    <div class="row">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">{{ text_my_product }}</h3>
        </div>
        <div class="panel-body">
          <div class="table-responsive">
            <table class="table table-bordered table-hover">
              <thead>
                <tr>
                  <td class="text-center">{{ column_image }}</td>
                  <td class="text-center">{{ column_product_name }}</td>
                  <td class="text-center">{{ column_status }}</td>
                  <td class="text-center">{{ column_sales}}</td>
                  <td class="text-center">{{ column_date_added }}</td>
                  <td class="text-center">{{ column_action }}</td>
                </tr>
              </thead>
              <tbody valign="middle" >
              
              {% if products %}
              {% for product in products %}
              <tr>
                <td class="text-center"><img src="{{ product.image }}" style="width: auto; height:100px;" alt="{{ product.name }}" class="img-thumbnail" /></td>
                <td class="text-center" style="vertical-align:middle; text-align:center; max-width:260px;">{{ product.name }}</td>
                <td class="text-center" style="vertical-align:middle; text-align:center;">{{ product.status }}</td>
                <td class="text-center" style="vertical-align:middle; text-align:center;">{{ product.sales }}</td>
                <td class="text-center" style="vertical-align:middle; text-align:center;">{{ product.published_at }}</td>
                <td class="text-center" style="vertical-align:middle; text-align:center;"> {% if product.view %} <a href="{{ product.view }}" target="_parent" data-toggle="tooltip" title="{{ button_view }}" class="btn btn-primary"><i class="fa fa-eye"></i>{{ button_view }}</a> {% else %}
                  <!--<div class="btn-group" >
                    <button type="button" onClick="addproduct();" id="pay{{order.order_id}}"   class="btn btn-success btn-xs" > {{ add_product }}</button>
                  </div>-->
                  <div class="btn-group"  style=" margin-left:20px;"> <a href="{{ product.href }}" target="_parent" data-toggle="tooltip" title="{{ button_edit }}" class="btn btn-primary"><i class="fa fa-pencil"></i> &nbsp;&nbsp; {{ button_edit }}</a> </div>
                  {% endif %} </td>
              </tr>
              {% endfor %}
              {% else %}
              <tr>
                <td class="text-center" colspan="8">{{ text_no_results }}</td>
              </tr>
              {% endif %}
                </tbody>
              
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
